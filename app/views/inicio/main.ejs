<script type="text/javascript">
      function navegador(){
          var agente = window.navigator.userAgent;
          var navegadores = ["Chrome", "Firefox", "Safari", "Opera", "Trident", "MSIE", "Edge"];
          for(var i in navegadores){
              if(agente.indexOf( navegadores[i]) != -1 ){
                  return navegadores[i];
              }
          }
      }
       function hablar(message) {
        var speech = new SpeechSynthesisUtterance();
        // Set the text and voice attributes.
        speech.text = message;
        speech.volume = 1;
        speech.rate = 0.9;
        speech.pitch = 1;
        var browser = document.getElementById("navegador");
        if (navegador()=="Firefox")
          speech.lang='es-ES_tradnl';
        else
          speech.lang='es-ES';
        window.speechSynthesis.speak(speech);
      }
       $('#btnCarga').on('click', function() {
        window.location.href='/';
      });
       $('#btnGauss').on('click', function() {
        window.location.href='/ley_de_gauss';
      });
       $('#btnPotencial').on('click', function() {
        window.location.href ='/potencial';
      });
       $('#btnCapacitancia').on('click', function() {
        window.location.href ='/capacitancia';
      });
       $('#btnSimulador').on('click', function() {
        window.location.href ='/simulador';
      });
       function Calcular(){
       	var k= 9*(Math.pow(10,9));
       	var q1=document.getElementById('txtq1').value;
       	var q2=document.getElementById('txtq2').value;
       	var r=document.getElementById('txtr').value;
       	var F=document.getElementById('txtResult').value;
       	if((q1.indexOf('e')!=-1) || (q1.indexOf('x10^')!=-1)){
       		q1=q1.replace('x10^','^');
       		var separador = q1.split(/[e^]/);
       		q1=Number(separador[0])*(Math.pow(10, Number(separador[1])));
       	}
       	if((q2.indexOf('e')!=-1) || (q2.indexOf('x10^')!=-1)){
       		q2=q2.replace('x10^','^');
       		var separador = q2.split(/[e^]/);
       		q2=Number(separador[0])*(Math.pow(10, Number(separador[1])));
       	}
       	if((r.indexOf('e')!=-1) || (r.indexOf('x10^')!=-1)){
       		r=r.replace('x10^','^');
       		var separador = r.split(/[e^]/);
       		r=Number(separador[0])*(Math.pow(10, Number(separador[1])));
       	}
       	if((F.indexOf('e')!=-1) || (F.indexOf('x10^')!=-1)){
       		F=F.replace('x10^','^');
       		var separador = F.split(/[e^]/);
       		F=Number(separador[0])*(Math.pow(10, Number(separador[1])));
       	}
       	if(F==0){
       		F=(k*(Math.abs(q1*q2)))/(Math.pow(r,2));
       		var Fs=(Number(F)).toPrecision(4); Fs=Fs.replace('e','x10^');
       		document.getElementById('txtResult').value=Fs;
       	}
       	if(q1==0){
       		q1=(F*(Math.pow(r,2)))/(k*(Math.abs(q2)));
       		var qs=(Number(q1)).toPrecision(4); qs=qs.replace('e','x10^');
       		document.getElementById('txtq1').value=qs;
       	}
       	if(q2==0){
       		q2=(F*(Math.pow(r,2)))/(k*(Math.abs(q1)));
       		var qs=(Number(q2)).toPrecision(4); qs=qs.replace('e','x10^');
       		document.getElementById('txtq2').value=qs;
       	}
       	if(r==0){
       		r=Math.sqrt((k*(Math.abs(q1*q2)))/F);
       		var rs=(Number(r)).toPrecision(4); rs=rs.replace('e','x10^');
       		document.getElementById('txtr').value=rs;
       	}
       }
    </script>
</body>
